#. full circle.
#: ../src/datain.c:924
#: ../src/datain.c:932
#: ../src/datain.c:944
#: n:51
msgid "Clino reading over 90 degrees (absolute value)"
msgstr "Absolútna hodnota sklonu väčšia ako 90 stupňov"

#: ../src/netbits.c:416
#: n:52
#, c-format
msgid "Tried to equate two non-equal fixed stations: “%s” and “%s”"
msgstr "Pokus o ztotožnenie dvoch rôznych fixných bodov “%s” a “%s”"

